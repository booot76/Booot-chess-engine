Function isAttackedby(color:Tcolor;square:Tsquare;var Board:Tboard):boolean;
var
  cp:integer;
  QR,QB:TBitBoard;
begin
 result:=false;
 if color=white then cp:=1 else  cp:=-1;
  if (PawnAttacksBB[color xor 1,square] and Board.Pieses[pawn*cp])<>0 then result:=true else
  if (KnightAttacksBB[square] and Board.Pieses[knight*cp])<>0 then result:=true else
  if (KingAttacksBB[square] and OnlyR00[Board.KingSq[color]])<>0 then result:=true else
    begin
      QB:=Board.Pieses[cp*bishop] or Board.Pieses[cp*queen];
      if ((BishopFullAttacksBB[square] and QB)<>0) and ((BishopMovesBB(square,Board) and QB)<>0)  then
        begin
          Result:=true;
          exit;
        end;
      QR:=Board.Pieses[cp*rook] or Board.Pieses[cp*queen];
      if ((RookFullAttacksBB[square] and QR)<>0) and ((RookMovesBB(square,Board) and QR)<>0) then Result:=true;
    end;
end;

Procedure SetCheckInfo(color:integer;var CheckInfo:TcheckInfo;var Board : TBoard); inline;
var
   cp:integer;
   opking,pinner : TSquare;
   QB,QR,temp,temp1 : TBitBoard;
begin
  
  // 1.     
  if Color=white then cp:=1 else cp:=-1;
  opking:=Board.KingSq[Color xor 1];
  CheckInfo.Direct[cp*pawn]:=PawnAttacksBB[color xor 1,opking];
  CheckInfo.Direct[cp*knight]:=KnightAttacksBB[opking];
  CheckInfo.Direct[cp*bishop]:=BishopMovesBB2(opking,Board);
  CheckInfo.Direct[cp*rook]:=RookMovesBB2(opking,Board);
  CheckInfo.Direct[cp*queen]:=CheckInfo.Direct[cp*rook] or CheckInfo.Direct[cp*bishop];
  Checkinfo.Direct[cp*king]:=0;
  // 2.  ( )    :
  CheckInfo.PinsOf[white]:=0;
  CheckInfo.PinsOf[black]:=0;
  //   :
  QR:=(Board.Pieses[BlackRook] or Board.Pieses[BlackQueen]) and RookFullAttacksBB[Board.KingSq[white]];
  QB:=(Board.Pieses[BlackBishop] or Board.Pieses[BlackQueen]) and BishopFullAttacksBB[Board.KingSq[white]];
  temp:=QR;
  while temp<>0 do
    begin
      pinner:=BitScanForward(temp);
      if (InterSect[Board.KingSq[white],pinner] and QR)=0 then
        begin
          temp1:=InterSect[Board.KingSq[white],pinner] and Board.AllPieses;
          if BitCount(temp1)=1 then CheckInfo.PinsOf[white]:=CheckInfo.PinsOf[white] or temp1;
        end;
      temp:=temp and NotOnlyR00[pinner];
    end;
  temp:=QB;
  while temp<>0 do
    begin
      pinner:=BitScanForward(temp);
      if (InterSect[Board.KingSq[white],pinner] and QB)=0 then
        begin
          temp1:=InterSect[Board.KingSq[white],pinner] and Board.AllPieses;
          if BitCount(temp1)=1 then CheckInfo.PinsOf[white]:=CheckInfo.PinsOf[white] or temp1;
        end;
      temp:=temp and NotOnlyR00[pinner];
    end;
  //   :
  QR:=(Board.Pieses[whiteRook] or Board.Pieses[whiteQueen]) and RookFullAttacksBB[Board.KingSq[black]];
  QB:=(Board.Pieses[whiteBishop] or Board.Pieses[whiteQueen]) and BishopFullAttacksBB[Board.KingSq[black]];
  temp:=QR;
  while temp<>0 do
    begin
      pinner:=BitScanForward(temp);
      if (InterSect[Board.KingSq[black],pinner] and QR)=0 then
        begin
          temp1:=InterSect[Board.KingSq[black],pinner] and Board.AllPieses;
          if BitCount(temp1)=1 then CheckInfo.PinsOf[black]:=CheckInfo.PinsOf[black] or temp1;
        end;
      temp:=temp and NotOnlyR00[pinner];
    end;
  temp:=QB;
  while temp<>0 do
    begin
      pinner:=BitScanForward(temp);
      if (InterSect[Board.KingSq[black],pinner] and QB)=0 then
        begin
          temp1:=InterSect[Board.KingSq[black],pinner] and Board.AllPieses;
          if BitCount(temp1)=1 then CheckInfo.PinsOf[black]:=CheckInfo.PinsOf[black] or temp1;
        end;
      temp:=temp and NotOnlyR00[pinner];
    end;
end;
 Function isPseudoLegal(color:integer;move:Tmove;var CheckInfo:TcheckInfo;var Board:Tboard):integer;inline;
 var
    from,dest:Tsquare;
    piese:Tpiese;
   begin
     from:=move and 63;
     dest:=(move shr 6) and 63;
     piese:=Board.Pos[from];
     //   - ,      ( ),      :
     if Board.oncheck then result:=hardmove else
     if (piese=WhiteKing) or (piese=BlackKing) then result:=hardmove else
     if ((move and CaptureFlag)<>0) and (Board.Pos[dest]=Empty) then result:=hardmove else
     //         -   
     if ((CheckInfo.PinsOf[color] and OnlyR00[From])<>0)  then
       begin
         if ((Intersect[Board.KingSq[color],From] and OnlyR00[Dest])<>0) then result:=moveislegal else
         if ((Intersect[Board.KingSq[color],Dest] and OnlyR00[From])<>0) then result:=moveislegal else result:=moveisunlegal;
       end else result:=moveislegal;
   end;
Function isMoveCheck(color:integer;move:Tmove;var CheckInfo:TCheckInfo;var Board:Tboard):boolean;
var
  from,dest : Tsquare;
  piese: Tpiese;
  Undo : Tundo;
 begin
   from:=move and 63;
   dest:=(move shr 6) and 63;
   piese:=Board.Pos[from];
   //  
   if (CheckInfo.Direct[piese] and OnlyR00[dest])<>0 then result:=true else
   //   (    ):
   if ((CheckInfo.PinsOf[color xor 1] and OnlyR00[From])<>0)  then
     begin
       //   -      :
       if ((Intersect[Board.KingSq[color xor 1],From] and OnlyR00[Dest])<>0) then result:=false else
       if ((Intersect[Board.KingSq[color xor 1],Dest] and OnlyR00[From])<>0) then result:=false else result:=true;
     end else
  //      ""    :
   //  
   if (((piese=WhiteKing) or (Piese=BlackKing)) and ((from-dest=2) or (from-dest=-2))) or
     //   
      (((move and CaptureFlag)<>0) and (Board.Pos[dest]=Empty)) or
    //  
     (((move shr 12) and 7)<>0) then
      begin
        InitNodeUndo2(Board,Undo);
        MakeMove(color,move,Board,Undo);
        result:=isAttackedBy(color,Board.KingSq[color xor 1],Board);
        UnMakeMove(color,Board,Undo);
      end else result:=false;
 end;
Function AllChecksBB(color:TColor;var Board:Tboard):TbitBoard;inline;
//    ,    color
var
   cp,myking:Tsquare;
begin
  if color=white then cp:=-1 else cp:=1;
  myking:=Board.KingSq[color];
  result:=(PawnAttacksBB[color,myking] and Board.Pieses[cp*pawn]) or (KnightAttacksBB[myking] and Board.Pieses[cp*knight])
       or (BishopMovesBB2(myking,Board) and (Board.Pieses[cp*bishop] or Board.Pieses[cp*queen])) or (RookMovesBB2(myking,Board) and (Board.Pieses[cp*rook] or Board.Pieses[cp*queen]));
end;
Function isMoveAttacks(var Board:Tboard;move:Tmove;sq:Tsquare):boolean;
var
   piese:Tpiese;
   dest:Tsquare;
   att : TBitBoard;
begin
  att:=0;
  dest:=(move shr 6) and 63;
  piese:=Board.Pos[move and 63];
  if Board.Color=black then piese:=-piese;
  case piese of
    Pawn   : att:=PawnAttacksBB[Board.Color,dest];
    knight : att:=KnightAttacksBB[dest];
    King   : att:=KingAttacksBB[dest];
    bishop : att:=BishopMovesBB(dest,Board);
    rook   : att:=RookMovesBB(dest,Board);
    queen  : att:=QueenMovesBB(dest,Board);
  end;
if (att and OnlyR00[sq])<>0 then result:=true else result:=false;
end;

